<!DOCTYPE html>
<html>
  <head>
    <title>Home Page</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
  <style>
     #menutren{
    margin : auto;
   }
   #myCarousel{
    margin : auto;
   }
  
  </style>
    </head>
  <body style="padding: 0;">
<!-- nav bar start-->
<div id="menutren" style="background: #e3f2fd;">
  <nav class="navbar navbar-light  ">
      <div class="container-fluid">
        <div class="nav-header " >
        <div class="navbar-header">
          <a class="navbar-brand" href="/">Smart farm</a>
        </div>
        <ul class="nav navbar-nav " style="background-color: #e3f2fd;"  >
          <% if (data.username) { %>
          <li><a href="/bieudonhietdo">Trang quản lý khu vườn</a></li>
  
          <% } else { %>
            <li><a href="/dangnhap">Đăng nhập</a></li>

            <% } %>
            <% if (data.role == 'admin') { %>
              <li><a href="/quanlynhanvien">Trang quản người dùng</a></li>
      
              <% }  %>
        </ul>
      </div>
    </div>
    </nav>
  </div>
    <!-- nav bar end -->
    <!-- carousel start -->
    <div id="myCarousel" class="carousel slide" data-ride="carousel" style="width:900px;">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1" ></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
    
      <!-- Wrapper for slides -->
      <div class="carousel-inner" style="height: 550px;overflow:hidden">
        <div class="item active">
          <img src="https://images.squarespace-cdn.com/content/v1/5d1d317669188500014755ff/1574092893039-HDFV3GW4FLA7RS2CVDBI/ke17ZwdGBToddI8pDm48kIZ80pXzhsplME5I17_dyqsUqsxRUqqbr1mOJYKfIPR7LoDQ9mXPOjoJoqy81S2I8N_N4V1vUb5AoIIIbLZhVYxCRW4BPu10St3TBAUQYVKcXa4tHz7eMGanDjEwPR9LJ1waP3CQFFieORTVTr7M93fXejltG4zwg4SLJuvzYoRu/Mobile+Background+SFI.jpg?format=2500w" alt="Los Angeles"  >
        </div>
    
        
        <div class="item">
          <img src="https://media.laodong.vn/Storage/NewsPortal/2020/1/19/779517/3.jpg" alt="New York" >
        </div>
      </div>
    
      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    <!-- carousel end -->
  </body>
</html>
